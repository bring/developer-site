        description: Your user's API key
      X-Bring-Client-URL:
        required: true
        description: Identifying the web-page or application where the Bring API data is used
    body:
        application/xml:
          examples:
            soap_request:
              displayName: SOAP
              value: !include examples/booking_soap_request.xml

