{
  "name": "developer-site",
  "version": "2.0.0",
  "description": "Bring developer site",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf _site",
    "buildassets": "webpack && rimraf static/css/main.js",
    "buildraml": "node scripts/raml2json",
    "build": "npm run buildassets && npm run buildraml",
    "start": "cross-env HUGO_ENV=dev hugo server",
    "buildqa": "npm run clean && npm run build && HUGO_ENV=qa hugo",
    "buildprod": "npm run clean && npm run build && HUGO_ENV=production hugo",
    "algolia": "atomic-algolia"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bring/developer-site.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bring/developer-site/issues"
  },
  "homepage": "https://developer.bring.com",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "atomic-algolia": "^0.3.19",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.9",
    "hugo-bin": "^0.77.0",
    "mini-css-extract-plugin": "^2.4.4",
    "postcss": "^8.3.11",
    "postcss-css-variables": "^0.18.0",
