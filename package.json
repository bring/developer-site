  "dependencies": {
    "algoliasearch": "^4.11.0",
    "react": "^17.0.2",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^17.0.2",
    "react-instantsearch-dom": "^6.8.1"
  }
