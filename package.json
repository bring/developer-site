{
  "name": "developer-site",
  "version": "2.0.0",
  "description": "Bring developer site",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf _site",
    "bundle": "webpack --mode production && rimraf static/css/main.js",
    "buildraml": "node scripts/raml2json",
    "buildexamples": "node scripts/buildApiExamples",
    "build": "npm run bundle && npm run buildraml && npm run buildexamples",
    "buildqa": "npm run clean && npm run build && HUGO_ENV=qa hugo",
    "buildprod": "npm run clean && npm run build && HUGO_ENV=production hugo",
    "start": "cross-env HUGO_ENV=dev hugo server --ignoreCache",
    "bundlewatch": "webpack watch --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bring/developer-site.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bring/developer-site/issues"
  },
  "homepage": "https://developer.bring.com",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "cssnano": "^5.1.0",
    "hugo-bin": "^0.80.2",
    "mini-css-extract-plugin": "^2.5.3",
    "postcss": "^8.4.6",
    "postcss-css-variables": "^0.18.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "raml-1-parser": "^1.1.67",
    "rimraf": "^3.0.2",
    "toml": "^3.0.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "browserslist": [
    "ie 11",
    "> .2%"
  ]
}
