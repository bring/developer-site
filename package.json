    "raml-1-parser": "^1.1.67",
    "rimraf": "^3.0.2",
    "toml": "^3.0.0",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  },
  "browserslist": [
    "ie 11",
    "> .2%"
