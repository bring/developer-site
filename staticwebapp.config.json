{
    "routes": [
        {
            "route": "/shipping-guide/demo",
            "redirect": "https://www.mybring.com/shipping-guide/demo"
        },
        {
            "route": "/shipping-guide/offline",
            "redirect": "https://www.mybring.com/shipping-guide/offline"
        },
        {
            "route": "/assets/bring-env/main.js",
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "route": "/assets/api-updates-subscribe/main.js",
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "route": "/assets/api-updates-subscribe/cr-script.js",
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
        }
    ],

    "globalHeaders": {
        "X-Frame-Options": "SAME-ORIGIN"
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/404.html"
        }
    },
    "platform": {
      "apiRuntime": "node:20"
    }
}
